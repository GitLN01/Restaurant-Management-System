@model List<DataAccessLayer.Models.Orders>

@{
    ViewData["Title"] = "Orders";
}

<h1>Orders</h1>
<table class="table table-bordered">
    <thead>
        <tr>
            <th>Order ID</th>
            <th>Customer ID</th>
            <th>Order Time</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var order in Model)
        {
            <tr>
                <td>@order.order_id</td>
                <td>@order.customer_id</td>
                <td>@order.time.ToString("yyyy-MM-dd HH:mm:ss")</td>
            </tr>
        }
    </tbody>
</table>
